{
  "rules": {
    "import/extensions": [
      "error",
      "always"
    ],
    "import/no-extraneous-dependencies": "off"
  },
  "ignorePatterns": [
    "/dist/"
  ],
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "root": true,
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "env": {
    "node": true,
    "es2021": true
  }
}
